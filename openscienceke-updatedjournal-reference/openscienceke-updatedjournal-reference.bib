	keywords = {phylogeography, Amazonia, cytochrome b, mammals, mtDNA},
	pages = {475--486},
	file = {Full Text PDF:files/367/Silva and Patton - 1998 - Molecular phylogeography and the evolution and con.pdf:application/pdf;Snapshot:files/368/j.1365-294x.1998.00276.html:text/html}
}

