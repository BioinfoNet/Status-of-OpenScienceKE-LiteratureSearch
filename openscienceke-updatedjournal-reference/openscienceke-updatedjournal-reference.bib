	pages = {490235},
	file = {Full Text PDF:files/768/Nogales et al. - 2018 - Fast and accurate large multiple sequence alignmen.pdf:application/pdf;Snapshot:files/769/490235v1.html:text/html}
}

@inproceedings{stamatakis_phylogenetic_2006,
	address = {Rhodes Island, Greece},
	title = {Phylogenetic models of rate heterogeneity: a high performance computing perspective},
	isbn = {978-1-4244-0054-6},
	shorttitle = {Phylogenetic models of rate heterogeneity},
	url = {http://ieeexplore.ieee.org/document/1639535/},
	doi = {10.1109/IPDPS.2006.1639535},
	abstract = {Inference of phylogenetic trees using the maximum likelihood (ML) method is NP-hard. Furthermore, the computation of the likelihood function for huge trees of more than 1,000 organisms is computationally intensive due to a large amount of ﬂoating point operations and high memory consumption. Within this context, the present paper compares two competing mathematical models that account for evolutionary rate heterogeneity: the Γ and CAT models. The intention of this paper is to show that—from a purely empirical point of view—CAT can be used instead of Γ. The main advantage of CAT over Γ consists in signiﬁcantly lower memory consumption and faster inference times. An experimental study using RAxML has been performed on 19 real-world datasets comprising 73 up to 1,663 DNA sequences. Results show that CAT is on average 5.5 times faster than Γ and—surprisingly enough—also yields trees with slightly superior Γ likelihood values. The usage of the CAT model decreases the amount of average L2 and L3 cache misses by factor 8.55.},
